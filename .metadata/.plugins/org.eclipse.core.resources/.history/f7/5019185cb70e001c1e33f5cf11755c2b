package Aula3;
import java.until.Scanner;
public class Atividade6 {
	public static void main(String[] args) {
		System.out.println("Qual seu nome?");
		java.util.Scanner s = new Scanner(System.in);
		var nome = s.nextDouble();
		System.out.println("Qual a sua qualidade?");
		var qualidade = s.nextDouble();
		
		System.out.println("Seja bem vindo, " + nome + ", sua qualidade princiapal é " + qualidade);
	}
	
}
